    "algorithms",
    "leetcode",
    "challenges"
  ],
  "author": "Felipe Rueda",
  "license": "ISC",
  "homepage": "https://github.com/ferueda/ts-leetcode#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "problems/*.ts"
    ]
